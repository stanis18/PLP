{ 
   var a  =  0 ,
   var b = "",
   var c = "aaa",
   var d = "bbb",
   
   var x =  "btes",
   var x1 = length x,
   
   proc incA (int z, int y)  {
     @pre z >= 0  y > 0 end 
     a := z / y
     @pos a >= 0 end
   },
   
   proc incB (string ss, string tt)  {
     
     @pre (length ss) > 1 end 
     
     b := ss ++ tt
     
     @pos ((length ss) + (length tt)) == 6 end
   };
   
   call incB("aaa", "bbb");
   write(b)
}